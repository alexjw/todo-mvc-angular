{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cool-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "../polyfills.ts",
      "tsconfig": "../tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "environmentSource": "environments/environment.ts"
    }
  ],
  "defaults": {
    "styleExt": "css"
  }
}